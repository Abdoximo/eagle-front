    "chart.js": "^4.4.1",
    "date-fns": "^3.3.1",
    "dompurify": "^3.2.6",
    "lucide-vue-next": "^0.344.0",
    "papaparse": "^5.4.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.19",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.3.0",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
